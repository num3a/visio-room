{
  "name": "visio-room",
  "private": true,
  "scripts": {
    "start": "meteor run",
    "start-visualizer": "meteor run --extra-packages bundle-visualizer --production",
    "start-c9-mlab": "meteor run --port $IP:$PORT --settings private/settings-c9.json",
    "start-mlab": "meteor run --settings private/settings.json",
    "deploy": "DEPLOY_HOSTNAME=galaxy.meteor.com meteor deploy app.visioroom.co --settings private/settings.json",
    "test": "meteor test --driver-package practicalmeteor:mocha --port 3002",
    "lint": "eslint .",
    "pretest": "npm run lint --silent"
  },
  "dependencies": {
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-polyfill": "^6.20.0",
    "babel-runtime": "^6.18.0",
    "bcrypt": "^0.8.7",
    "classnames": "^2.2.5",
    "enzyme": "^2.9.1",
    "from": "^0.1.3",
    "history": "^4.6.1",
    "i18next": "^8.4.3",
    "i18next-browser-languagedetector": "^1.0.1",
    "i18next-localstorage-cache": "^1.1.1",
    "i18next-xhr-backend": "^1.5.0",
    "import": "0.0.6",
    "lodash": "^4.17.4",
    "meteor-node-stubs": "~0.2.0",
    "moment": "^2.18.1",
    "prop-types": "^15.5.7",
    "react": "^16.0.0",
    "react-addons-pure-render-mixin": "^15.6.2",
    "react-dates": "^10.2.0",
    "react-dom": "^16.0.0",
